{
  "name": "baekya-protocol",
  "version": "1.0.0",
  "description": "독립적인 블록체인 사회시스템 - 기여기반 탈중앙 프로토콜",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "start": "node src/index.js",
    "mainnet": "cross-env NODE_ENV=production node src/index.js --mainnet",
    "testnet": "node src/index.js --testnet",
    "validator": "node src/index.js --validator",
    "demo": "node demo.js",
    "interactive": "node interactive-demo.js",
    "server": "node server.js",
    "relay": "node p2p-relay-server.js",
    "mobile": "node mobile-api-server.js",
    "build": "webpack --mode production",
    "dev": "webpack serve --mode development",
    "chain:start": "node src/blockchain/node.js",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --ext .js,.ts --fix",
    "cap:sync": "npx cap sync",
    "cap:add:android": "npx cap add android",
    "cap:add:ios": "npx cap add ios",
    "cap:run:android": "npx cap run android",
    "cap:run:ios": "npx cap run ios",
    "cap:open:android": "npx cap open android",
    "cap:open:ios": "npx cap open ios",
    "mobile:build": "npm run build && npx cap sync",
    "mobile:android": "npm run mobile:build && npx cap run android",
    "mobile:ios": "npm run mobile:build && npx cap run ios"
  },
  "dependencies": {
    "@capacitor/camera": "^7.0.1",
    "@capacitor/filesystem": "^7.1.1",
    "@capacitor/share": "^7.0.1",
    "axios": "^1.10.0",
    "capacitor-biometric-auth": "^0.1.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "elliptic": "^6.5.4",
    "express": "^4.18.2",
    "firebase": "^11.10.0",
    "firebase-admin": "^13.4.0",
    "js-sha256": "^0.9.0",
    "level": "^8.0.0",
    "localtunnel": "^2.0.2",
    "merkletreejs": "^0.3.10",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.1",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@capacitor/android": "^7.4.0",
    "@capacitor/cli": "^7.4.0",
    "@capacitor/core": "^7.4.0",
    "@capacitor/ios": "^7.4.0",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.54.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "keywords": [
    "blockchain",
    "dao",
    "defi",
    "governance",
    "contribution-based",
    "social-system"
  ],
  "author": "Baekya Protocol Team",
  "license": "MIT"
}
